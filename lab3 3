
#include <stdio.h>
#include <math.h>

int main()
{
    float x,f,y,a,b;
    printf("x=");
    scanf("%f",&x);
    printf("y=");
    scanf("%f",&y);
    a=pow(x,2)+pow(y,2);
    b=sqrt(x-y);
    if (b<=0)
    {
        printf("У відповіді є від'ємне число ");
    }
    else
    {
        f=a/b;
        printf("a = %f",f);
            
        
    }
    
}
